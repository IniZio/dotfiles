        -e 's/(0\x6D(B/|/g' \
        -e 's/(0\x6E(B/-/g' \
        -e 's/(0\x74(B/|/g' \
        -e 's/(0\x75(B/-/g' \
        -e 's/(0\x76(B/|/g' \
        -e 's/(0\x77(B/-/g' \
        -e 's/(0\x78(B/|/g' \
