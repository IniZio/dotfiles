{
    "window.menuBarVisibility": "toggle",
    "workbench.activityBar.visible": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.visible": false,
    "editor.fontSize": 16,
    "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'Courier New', monospace, 'Droid Sans Fallback'",
    "editor.tabSize": 2,
    "files.exclude": {
        "**/.nuxt": true,
        "**/.git": true,
        "**/node_modules": true,
        "**/bower_modules": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/dist": true,
        "**/*.js.map": true,
        "**/*.min.js": true,
        "**/.cache": true
    },
    "editor.minimap.enabled": false,
    "workbench.colorTheme": "Nord",
    "todohighlight.isCaseSensitive": false,
    "window.zoomLevel": 0,
    "files.insertFinalNewline": true,
    "vim.disableAnnoyingNeovimMessage": true,
    "vim.useCtrlKeys": false,
    "explorer.openEditors.visible": 0,
    "vim.useSystemClipboard": true,
    "php.suggest.basic": false,
    "eslint.autoFixOnSave": true,
    "editor.smoothScrolling": true,
    "editor.lineHeight": 22,
    "gitlens.recentChanges.file.lineHighlight.locations": [
        "line",
        "gutter",
        "overviewRuler"
    ],
    "git.enableCommitSigning": true,
    "C_Cpp.intelliSenseEngine": "Default",
    "emmet.includeLanguages": {
        "vue-html": "html",
        "javascript": "javascriptreact"
    },
    "prettier.eslintIntegration": true,
    "prettier.stylelintIntegration": true,
    "prettier.singleQuote": true,
    "prettier.semi": false,
    "search.useIgnoreFilesByDefault": true,
    "java.errors.incompleteClasspath.severity": "ignore",
    "flow.fileExtensions": [
        ".js",
        ".vue",
        ".mjs",
        ".jsx",
        ".flow",
        ".json"
    ],
    "typescript.npm": "~/.nvm/versions/node/v8.7.0/bin/npm",
    "typescript.check.npmIsInstalled": false,
    "javascript.validate.enable": false,
    "workbench.panel.location": "bottom",
    "editor.renderIndentGuides": false,
    "eslint.run": "onSave",
    "flow.runOnEdit": false,
    "flow.enabled": false,
    "gitlens.gitExplorer.files.layout": "auto",
    "fileheader.Author": "Newman",
    "fileheader.LastModifiedBy": "Newman",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "eslint.packageManager": "yarn",
    "php.memoryLimit": "1000M",
    "git.autofetch": true
}
